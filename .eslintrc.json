{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "next/core-web-vitals",
    "plugin:@next/next/recommended",
    "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [ "prettier"],
  "rules": {
    "react/display-name": 0,
    "jsx-quotes":[2, "prefer-single"],
    "no-plusplus": "off",
    "react/jsx-props-no-spreading": "off"
  }
}
